vars.put(&quot;firstName&quot;, firstNameArr[randomIndex]);

${__Random(0,190, pageNum)};

String[] pageSizeArr = {&quot;1000&quot;, &quot;2000&quot;, &quot;3000&quot;, &quot;4000&quot;, &quot;5000&quot;};
int randomIndex2 = org.apache.commons.lang3.RandomUtils.nextInt(0, pageSizeArr.length);
vars.put(&quot;pageSize&quot;, pageSizeArr[randomIndex2]);
</stringProp>
